	<!--快花花快钱配置-->
	<strategy>
		<mpf>
			<!-- 商户会员号 -->
			<commons:member-code>10210634137</commons:member-code>
			<!-- 策略编码，固定值 F41-->
			<commons:feature-code>F41</commons:feature-code>
		</mpf>
		<crypto-strategy>
			<type>signatureAndCrypto</type>
			<available>true</available>
			<algorithm-set>
				<signature-alg>SHA1withRSA</signature-alg>
				<symmetric-alg>AES/CBC/PKCS5Padding</symmetric-alg>
				<asymmetric-alg>RSA/ecb/pkcs1Padding</asymmetric-alg>
			</algorithm-set>
		</crypto-strategy>
		<key-strategy>
			<!-- 快钱公钥路径-->
			<bill99-public-key-file-path>kuaiqian/cer/99bill.cert.rsa.20340630.cer</bill99-public-key-file-path>
			<mpf-keystore>
				<!-- 商户私钥路径-->
				<path>kuaiqian/quick/quick-rsa.pfx</path>
				<key-store-type>PKCS12</key-store-type>
				<!-- 商户私钥密码-->
				<key-store-password>quick123</key-store-password>
				<!-- 商户私钥别名-->
				<alias>test-alias</alias>
				<!-- 商户私钥密码-->
				<key-password>quick123</key-password>
			</mpf-keystore>
		</key-strategy>
	</strategy>
